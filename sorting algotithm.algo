ALGORITHM algorithm_Insertion sort

VAR
    i,j,n;INTEGER
BEGIN
FOR i FROM 1 TO j-1 STEP 1  DO
    IF (i =1; i<n, i++) THEN
            let current = inputArr[i];
    END_IF  
 j = i-1; 
 WHILE (j > - 1 && (current < inputArr[j])) DO
                     inputArr[j+1] = inputArr[j];
            inputArr[j+1] = current;
    return inputArr;

 END_WHILE
END_FOR
END